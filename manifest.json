{
    "name": "OctoTicket",
    "description": "Will it work? find out!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "storage"
    ],
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "content_scripts": [
        {
            "matches" : ["https://app.asana.com/*"],
            "js" : ["asana-ticket.js"]
        }
    ]
}
